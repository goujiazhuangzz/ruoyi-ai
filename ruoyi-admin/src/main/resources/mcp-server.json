{
  "mcpServers": {
    "时间": {
      "command": "uvx",
      "args": [
        "mcp-server-time"
      ],
      "env": {}
    },
    "redis": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-redis",
        "redis://127.0.0.1:6379"
      ],
      "env": {}
    },
    "everything": {
      "command": "uvx",
      "args": [
        "mcp-server-everything-search"
      ],
      "env": {}
    },
    "fileSystem": {
      "command": "/usr/local/bin/npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/liuzhiqiang/"
      ],
      "env": {}
    },
    "search1api": {
      "command": "/usr/local/bin/npx",
      "args": [
        "-y",
        "search1api-mcp"
      ],
      "env": {
        "SEARCH1API_KEY": "xx"
      }
    }
  }
}